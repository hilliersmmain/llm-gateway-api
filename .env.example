# Gemini Guardrails API Environment Variables

# Required: Your Google Gemini API Key
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Database connection URL (default: local Docker PostgreSQL)
DATABASE_URL=postgresql+asyncpg://user:password@db:5432/llm_gateway

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
